<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="Pull-from-KVM" mapIdentifier="Ubiq">
    <DisplayName>Pull from KVM</DisplayName>
    <Properties/>
    <!-- private. is required for Encrypted KVM data -->
    <Get assignTo="private.dataset_def">
        <Key>
            <!-- ex: BIRTH_DATE.definition -->
            <Parameter ref="def_variable"/>
        </Key>
    </Get>
    <Get assignTo="private.dataset_key">
        <Key>
            <!-- ex: BIRTH_DATE.key -->
            <Parameter ref="key_variable"/>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>