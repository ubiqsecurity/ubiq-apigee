<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
    <Step>
        <Name>KVM-Keys</Name>
    </Step>
    <Step>
        <Name>Pull-from-KVM</Name>
    </Step>
    <Step>
        <Name>Definition-Management</Name>
    </Step>
    <Step>
        <Condition>(private.dataset_def is null) or (private.dataset_key is null)</Condition>
        <Name>Create-Request</Name>
    </Step>
    <Step>
        <Condition>(private.dataset_def is null) or (private.dataset_key is null)</Condition>
        <Name>Ubiq-Auth</Name>
    </Step>
    <Step>
        <Condition>(private.dataset_def is null) or (private.dataset_key is null)</Condition>
        <Name>Get-Dataset-Keys-and-Def</Name>
    </Step>
    <Step>
        <Name>Handle-Variables</Name>
    </Step>
    <Step>
        <Condition>(private.dataset_def IsNot null) and (responseDatasetKey IsNot null)</Condition>
        <Name>Add-Definition-to-KVM</Name>
    </Step>
    <Step>
        <Condition>(private.dataset_key IsNot null) and (responseDatasetKey IsNot null)</Condition>
        <Name>Add-Key-to-KVM</Name>
    </Step>
</SharedFlow>