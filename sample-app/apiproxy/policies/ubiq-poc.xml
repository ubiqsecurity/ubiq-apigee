<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<JavaCallout name="ubiq-poc">
    <ClassName>com.apigeesample.UbiqPoc</ClassName>
     <Properties>
         <Property name="DatasetsMappings">[
   { "dataset" : "BIRTH_DATE", "json_path" : "$..birthDate"},
   { "dataset" : "SSN", "json_path" : "$..ssn"}
]</Property>
        <!-- Valid inputs: NANOS, MILLIS, SECONDS, MINUTES, HOURS, HALF_DAYS, DAYS -->
        <Property name="usage_timestamp_reporting_granularity">HOURS</Property>
     </Properties>
    <ResourceURL>java://edge-custom-policy-ubiq-poc.jar</ResourceURL>
    <DisplayName>Ubiq POC</DisplayName>
</JavaCallout>
